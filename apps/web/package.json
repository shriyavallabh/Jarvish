{
  "name": "@project-one/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:puppeteer": "cd puppeteer-tests && jest --config jest.config.js",
    "test:puppeteer:headed": "cd puppeteer-tests && HEADLESS=false jest --config jest.config.js",
    "test:puppeteer:debug": "cd puppeteer-tests && HEADLESS=false DEVTOOLS=true jest --config jest.config.js --runInBand",
    "test:puppeteer:update-baseline": "cd puppeteer-tests && UPDATE_BASELINE=true jest --config jest.config.js",
    "test:a11y": "cd puppeteer-tests && jest --config jest.config.js --testNamePattern='accessibility'",
    "test:visual": "cd puppeteer-tests && jest --config jest.config.js --testNamePattern='Visual Regression'",
    "test:perf": "cd puppeteer-tests && jest --config jest.config.js tests/performance.test.ts"
  },
